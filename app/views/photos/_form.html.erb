<%= form_for @photo, html: { multipart: true }  do |f| %>
  <% if @photo.errors.any? %>
  <div id ="errors">
    <h2><%= pluralize(@photo.errors.count, "error") %> prevented this post from saving. </h2>
    <ul>
      <% @photo.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
    </ul>
  </div>
  <% end %>

  <p>
    <%= f.label :image %><br>
    <%= f.file_field :image %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>
